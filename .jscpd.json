{
  "threshold": 0,
  "reporters": ["console", "json"],
  "output": ".jscpd-report",
  "ignore": [
    "**/node_modules/**",
    "**/chobble-template/**",
    "**/coverage/**",
    "**/package-lock.json",
    "**/.jscpd-report/**",
    "**/test/**"
  ],
  "format": ["javascript"],
  "absolute": false,
  "gitignore": true,
  "exitCode": 1,
  "minLines": 5,
  "minTokens": 50
}
